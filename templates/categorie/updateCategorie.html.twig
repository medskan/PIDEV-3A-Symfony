{% extends 'base.html.twig' %}
{% block title %}
    <h1>Modifier Produit</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="{{ asset('base-back.html.twig')}}">Home</a></li>
            <li class="breadcrumb-item active">Modifier Produit</li>
        </ol>
    </nav>
{% endblock %}
{%  block body %}
    {{ form_start(f, {'attr': {'novalidate': 'novalidate'}}) }}
    <table>
        <tr>  <td> {{ form_label(f.nomC) }}</td>
            <td> {{ form_widget(f.nomC) }}</td>
            <td>{{ form_errors(f.nomC) }}</td>
        </tr>

        <tr>  <td>{{ form_label(f.type) }}</td>
            <td>  {{ form_widget(f.type) }}</td>
            <td> {{ form_errors(f.type) }}</td>
        </tr>

    </table>
    {{ form_end(f) }}

{% endblock %}
