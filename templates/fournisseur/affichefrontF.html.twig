{% extends 'front.html.twig' %}

{% block body %}
    <section class="services-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <span>Nos Fournisseurs</span>
                        <h2>PUSH YOUR LIMITS FORWARD</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <tbody>
                {% for i in f %}
                <div class="col-lg-3 order-lg-1 col-md-6 p-0">
                    <div class="ss-pic">
                        <img src="{{ asset('/uploads/' ~ i.image ) }}" alt="img" class="image" style="max-height: 200px; width: 200px;">
                    </div>
                    <div class="col-lg-3 order-lg-2 col-md-6 p-0">
                        <div class="ss-text">
                            <h4>{{ i.nomF }}</h4>
                            <h3>{{ i.prenomF }}</h3>
                            <p>{{ i.adresse}}</p>
                            {# <a href="#">Explore</a> #}
                            <p>{{ i.telF}}</p>
                        </div>
                </div>


                </div>
                {% endfor %}
                </tbody>
            </div>
        </div>
    </section>

{% endblock %}